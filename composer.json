{
    "name": "skytells/cache",
    "description": "The Skytells Cache package.",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "skytells/contracts": "5.4.*",
        "skytells/support": "5.4.*",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Skytells\\Cache\\": ""
        },
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "suggest": {
        "skytells/database": "Required to use the database cache driver (5.4.*).",
        "skytells/filesystem": "Required to use the file cache driver (5.4.*).",
        "skytells/redis": "Required to use the redis cache driver (5.4.*)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
